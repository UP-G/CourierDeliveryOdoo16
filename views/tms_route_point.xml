<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_tms_route_point_tree" model="ir.ui.view">
        <field name="name">tms.route.point.tree</field>
        <field name="model">tms.route.point</field>
        <field name="arch" type="xml">
            <tree string="Tms Route Sheet">
                <field name="id"/>
                <field name="res_partner_id"/>
                <field name="route_id"/>
            </tree>
        </field>
    </record>

    <record id="action_tms_route_point_tree" model="ir.actions.act_window">
        <field name="name">tms.route.sheet</field>
        <field name="res_model">tms.route.point</field>
        <field name="view_mode">tree,form</field>
<!--        <field name="view_id" ref="view_tms_route_point_tree"/>-->
    </record>

    <record id="view_tms_route_point_form" model="ir.ui.view">
        <field name="name">tms.route.point.form</field>
        <field name="model">tms.route.point</field>
        <field name="arch" type="xml">
            <form string="Tms form">
                <sheet>
                    <group>
                        <field name="id"/>
                        <field name="res_partner_id"/>
                        <field name="route_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

<!--    <record id="action_tms_route_point_form" model="ir.actions.act_window">-->
<!--        <field name="name">tms.route.point.add</field>-->
<!--        <field name="res_model">tms.route.point</field>-->
